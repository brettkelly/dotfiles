    "workbench.editor.empty.hint": "hidden",
    "workbench.settings.applyToAllProfiles": [],
    "intelephense.files.include": [
        "**/*.php",
        "wp-content/plugins/elementor/**/*.php",
        "wp-content/themes/**/*.php"
    ],
    "intelephense.files.exclude": [
        "**/node_modules/**",
        "**/vendor/**",
        "**/cache/**"
    ]
}